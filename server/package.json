{
  "name": "twitter-exporter-server",
  "version": "1.0.0",
  "description": "Server application",
  "author": "",
  "license": "MIT",
  "bin": "dist/server/src/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --config nest.json",
    "package": "pkg . --output=${DEST_DIR:=.}/$npm_package_name-${VERSION:=$npm_package_version}.bin",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start --config nest.json",
    "start:dev": "cross-env NODE_ENV=development nest start --watch --config nest.json",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch --config nest.json",
    "start:prod": "cross-env NODE_ENV=production node dist/server/src/main",
    "prestart:prod": "npm run build",
    "check-types": "tsc",
    "check": "npm run check-types && npm run lint",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "pkg": {
    "targets": [
      "node12-linux-x64"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/swagger": "^4.5.3",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "mongoose": "^5.9.20",
    "nest-logger": "^5.0.1",
    "nestjs-session": "^1.0.1",
    "passport": "^0.4.1",
    "passport-twitter": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "twitter-lite": "^0.14.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.9",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.1",
    "@types/mongoose": "^5.7.27",
    "@types/node": "^13.13.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "pkg": "^4.4.8",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "rootDir": "./src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
